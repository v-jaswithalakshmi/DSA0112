#include <iostream>
using namespace std;

// Function to calculate Fibonacci number recursively
int fib(int n) {
    if (n == 1) {
        return 0;
    } else if (n == 2) {
        return 1;
    } else {
        return fib(n-1) + fib(n-2);
    }
}

int main() {
    int n;
    cout << "Enter the value of N: ";
    cin >> n;

    cout << "Fibonacci number is " << fib(n) << endl;

    return 0;
}
